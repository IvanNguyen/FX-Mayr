section.teams
    .container
        form.select-tab
            select#my-select
                option(value='0') Executive Team
                option(value='1') Retreats Executive Teams
                option(value='2') Clubs Executive Teams
                option(value='3') International Experts
                option(value='4') Partner & Sponsors
            .arrow
        ul#myTab.pills-tab.nav.nav-pills(role='tablist')
            li.nav-item
                a#pills-executive-team-tab.nav-link.active(data-toggle='pill', href='#pills-executive-team', role='tab', aria-controls='pills-executive-team', aria-selected='true',title="executive team") Executive Team
            li.nav-item
                a#pills-retreats-executive-teams-tab.nav-link(data-toggle='pill', href='#pills-retreats-executive-teams', role='tab', aria-controls='pills-retreats-executive-teams', aria-selected='false',title="retreats teams") Retreats Executive Teams
            li.nav-item
                a#pills-clubs-executive-teams-tab.nav-link(data-toggle='pill', href='#pills-clubs-executive-teams', role='tab', aria-controls='pills-clubs-executive-teams', aria-selected='false',title="clubs executive teams") Clubs Executive Teams
            li.nav-item
                a#pills-international-experts-tab.nav-link(data-toggle='pill', href='#pills-international-experts', role='tab', aria-controls='pills-international-experts', aria-selected='false',title="international experts") International Experts
            li.nav-item
                a#pills-partner-sponsors-tab.nav-link(data-toggle='pill', href='#pills-partner-sponsors', role='tab', aria-controls='pills-partner-sponsors', aria-selected='false',title="partner & sponsors") Partner & Sponsors
        #pills-tabContent.tab-content
            #pills-executive-team.tab-pane.fade.show.active(role='tabpanel', aria-labelledby='pills-executive-team-tab')
