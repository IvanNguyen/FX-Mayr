                            h5.people__name Olaf Schulz-Lobeck
                            p.people__job
                                | Chairman & President
                            button.toggle-detail.d-md-none(data-toggle='collapse',data-target='#collapseItem01', aria-expanded='true', aria-controls='collapseItem01')
